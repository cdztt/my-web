{
  "name": "mern-ssr-webpack",
  "version": "1.0.0",
  "description": "A MERN web app built with webpack.",
  "type": "module",
  "scripts": {
    "build:client": "cross-env NODE_ENV=development webpack watch --config webpack.client.config.cjs --env development",
    "build:server": "cross-env NODE_ENV=development webpack watch --config webpack.server.config.cjs --env development",
    "build:production:client": "cross-env NODE_ENV=production webpack --config webpack.client.config.cjs --env production",
    "build:production:server": "cross-env NODE_ENV=production webpack --config webpack.server.config.cjs --env production",
    "build": "npm run build:client && npm run build:server",
    "build:production": "npm run build:production:client && npm run build:production:server",
    "start": "nodemon ./dist/server.cjs",
    "dev": "cross-env NODE_ENV=development webpack serve --config webpack.localhostDev.config.cjs --env development --env localhostDev",
    "serve": "node ./localServer.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@cdztt/message-react": "^1.1.3",
    "@cdztt/tooltip-react": "^1.2.7",
    "@reduxjs/toolkit": "^1.8.3",
    "antd": "^4.22.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^6.0.3",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "babel-loader": "^8.2.5",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "debug": "^4.3.4",
    "eslint": "^8.21.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.1",
    "mongodb": "^4.8.1",
    "sass": "^1.54.2",
    "sass-loader": "^13.0.2",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  }
}
